$(document).ready(function(){
	
	
	$("#headerTitleHome").click(function(){
		window.location.href = "http://localhost:8282/insurance/home";
	})
	
	const myTimeout = setTimeout(myGreeting, 5000);

	function myGreeting() {
	  window.location.href = "http://localhost:8282/insurance/home";
	}
	
	function myStopFunction() {
	  clearTimeout(myTimeout);
	}
	
});